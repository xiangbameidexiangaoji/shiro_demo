  <select id="selectFindById" resultMap="perms" parameterType="java.lang.Long">
        SELECT
        u.username,
        u.mobile,
        u.email,
        u.password,
        u.role_id,
        u.name,
        u.job,
        u.id_num,
        admin_role.NAME AS 'roleName',
        admin_role.description AS 'roleDescription',
        admin_permission.perms AS 'perms',
        admin_permission.menu_name AS 'menu_name',
        admin_permission.description AS 'permsDescription'
    FROM
        admin_user AS u
        INNER JOIN admin_role ON u.role_id = admin_role.id
        INNER JOIN admin_role_permission ON admin_role_permission.role_id = admin_role.id
        INNER JOIN admin_permission ON admin_permission.id = admin_role_permission.permission_id
    WHERE
        u.id = #{id}
  </select>
